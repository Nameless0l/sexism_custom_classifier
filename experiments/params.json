{
    "data_file":"/path/data.csv",
    "iteration_count":1,
    "train_domains":[{ "items":["BHO", "BHOM", "BHOCS", "BHOCSM", "C", "CM"], "__domain__": true}],
    "test_domains":[{ "items":["BHO", "BHOM", "BHOCS", "BHOCSM", "C", "CM", "S"], "__domain__": true}],
    "models":[{ "items":["LR", "SVM", "CNN"], "__model__": true}],
    "features":
    {
        "1": 
        { 
            "name":{ "item":"SENTIMENT", "__feature__": true},
            "feature_selection":false,
            "param_grid":{ "features__sentiment__feature_extraction__score_names": [["neg", "compound", "neu", "pos"]] }
        },
        "2": 
        { 
            "name":{ "item":"NGRAM", "__feature__": true},
            "feature_selection":false,
            "param_grid": { "features__ngram__feature_extraction__ngram_range": [{ "items":[1, 1], "__tuple__": true}] }
        },
        "3": 
        {
            "name":{ "item":"NGRAM", "__feature__": true},
            "feature_selection":false,
            "param_grid": { "features__ngram__feature_extraction__ngram_range": [{ "items":[1, 2], "__tuple__": true}] }
        },
        "4":
        {
            "name":{ "item":"TYPEDEPENDENCY", "__feature__": true},
            "feature_selection":false,
            "param_grid": {
                "features__type_dependency__feature_extraction__ngram_range": [{ "items":[1, 1], "__tuple__": true}],
                "features__type_dependency__feature_extraction__model_path": ["/path/stanford-corenlp-4.1.0"]
            }
        },
        "5":
        {
            "name":{ "item":"BERTDOC", "__feature__": true},
            "feature_selection":false,
            "param_grid": {
                "features__bert_doc__feature_extraction__embedding_file_name": ["/path/doc_embeddings.pkl"]
            }
        },
        "6":
        {
            "name":{ "item":"BERTWORD", "__feature__": true},
            "feature_selection":false,
            "param_grid": {
                "features__bert_word__feature_extraction__embedding_file_name": ["/path/word_embeddings.pkl"]
            }
        },
        "7":
        {
            "name":{ "item":"TEXTVEC", "__feature__": true},
            "feature_selection":false,
            "param_grid": {
                "features__textvec__feature_extraction__max_tokens": [2400],
                "features__textvec__feature_extraction__output_sequence_length": [60]
            }
        }
    }
}